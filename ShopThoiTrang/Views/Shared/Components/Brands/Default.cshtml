@model IEnumerable<BrandModel>
@foreach(var item in Model)
{
    <div>
        <div>
            <h4 class="panel-heading">
                <a asp-actions="Index" asp-controller="Brand" asp-route-Slug="@item.Slug">
                     @item.Name
                </a>
            </h4>
        </div>
    </div>
}